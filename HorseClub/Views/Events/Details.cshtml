@model HorseClub.Models.Events

@{
    ViewData["Breed"] = "Event Details";
}
@{ Layout = "_Layout"; }
<br />
<div>
    <h4>Event Details</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.EventGroup)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.EventGroup)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Time)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Time)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Message)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Message)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id" type="button" class="btn btn-info">Edit</a> |
    <a asp-action="Index" type="button" class="btn btn-info">Back to List</a>
</div>
